{
    "name": "mohatt/github-api-utils",
    "type": "library",
    "description": "Utility library for Github API authentication, token switching and static repository analysis",
    "homepage": "https://github.com/mohatt/github-api-utils",
    "keywords": ["github", "gh", "api", "utils"],
    "license": "MIT",
    "authors": [
    	{
    		"name": "Mohamed Elkholy",
    		"email": "mohatt@pm.me"
    	}
    ],
    "require": {
        "php": "^8.0",
        "psr/log": "^1.0",
        "php-http/discovery": "^1.13",
        "psr/http-client-implementation": "^1.0",
        "psr/http-factory-implementation": "^1.0",
        "php-http/client-common": "^2.3",
        "knplabs/github-api": "^3.3"
    },
    "require-dev": {
        "symfony/dom-crawler": "^3.1",
        "symfony/css-selector": "^3.1",
        "guzzlehttp/guzzle": "^7.1",
        "guzzlehttp/psr7": "2.0.0-rc1"
    },
    "suggest": {
        "symfony/dom-crawler": "Used by Github Inspector class",
        "symfony/css-selector": "Used by Github Inspector class"
    },
    "autoload": {
        "psr-4": {
        	"Github\\Utils\\": "src/"
    	}
    }
}
